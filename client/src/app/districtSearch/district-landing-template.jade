.container(style="padding-top: 20px;")
  h4.text-center {{district.state | stateFilter}} District {{district.districtNumber}}

  .district-container
    div 
      h5 House Representative
        ul.no-bullet
          li
            a(ng-bind="houseRep | congressmanNameFilter", ui-sref="congressman({id: houseRep.bioguide_id})")
         
    div 
      h5 Senators 
        ul.no-bullet
          li
            a(ng-bind="senators[0] | congressmanNameFilter", ui-sref="congressman({id: senators[0].bioguide_id})")
          li
            a(ng-bind="senators[1] | congressmanNameFilter", ui-sref="congressman({id: senators[1].bioguide_id})")